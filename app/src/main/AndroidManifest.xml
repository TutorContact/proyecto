<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nathali.tutorcontactapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginTutor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PantallaPrincipalTutor" />
        <activity
            android:name=".RegistroTutor"
            android:parentActivityName=".LoginTutor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nathali.tutorcontactapp.LoginTutor" />
        </activity>
        <activity android:name=".horario_disponible" />

    </application>

</manifest>